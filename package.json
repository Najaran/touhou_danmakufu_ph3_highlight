{
    "name": "touhou-danmakufu-ph3-highlight",
    "displayName": "Touhou Danmakufu ph3 Highlight",
    "description": "Touhou danmakufu ph3 script(.dnh) Syntax Highlight for Visual Studio Code.",
    "version": "0.0.1",
    "publisher": "OTA",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dnh",
            "aliases": ["Danmakufu", "dnh"],
            "extensions": [".dnh"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dnh",
            "scopeName": "source.dnh",
            "path": "./syntaxes/dnh.tmLanguage.json"
        }]
    }
}